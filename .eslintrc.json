{
  "extends": ["airbnb"],
  "plugins": ["react", "import", "jsx-a11y"],
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
  },
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "rules": {
    "indent": ["error", "tab"],
    "quotes": ["error", "double"],
    "semi": ["error", "never"],
    "no-tabs": [0],
    "function-paren-newline": [0],
    "global-require": [0],
    "no-console": [0],
    "func-names": [0],
    "no-param-reassign": [0],
    "import/no-dynamic-require": [0],
    "import/first": [0],
    "consistent-return": [0],
    "import/newline-after-import": [0],
    "linebreak-style": ["error", "windows"],
    "react/prefer-stateless-function": [0],
    "react/jsx-filename-extension": [0],
    "react/jsx-indent": [0],
    "react/jsx-indent-props": [0],
    "linebreak-style": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "no-plusplus": [0],
    "max-len": [0]
  }
}
